{
  "name": "nuxt4-app",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development nuxt dev",
    "dev:uat": "NODE_ENV=uat nuxt dev",
    "build": "NODE_ENV=production nuxt build",
    "build:uat": "NODE_ENV=uat nuxt build",
    "generate": "NODE_ENV=production nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "NODE_ENV=development bun run scripts/migrate.ts",
    "db:migrate:uat": "NODE_ENV=uat bun run scripts/migrate.ts",
    "db:migrate:prod": "NODE_ENV=production bun run scripts/migrate.ts",
    "db:push": "NODE_ENV=development drizzle-kit push",
    "db:push:uat": "NODE_ENV=uat drizzle-kit push",
    "db:push:prod": "NODE_ENV=production drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:seed": "NODE_ENV=development bun run scripts/seed.ts",
    "db:seed:uat": "NODE_ENV=uat bun run scripts/seed.ts",
    "db:setup": "bun run db:migrate && bun run db:seed",
    "db:setup:uat": "bun run db:migrate:uat && bun run db:seed:uat",
    "db:setup:prod": "bun run db:migrate:prod",
    "api": "NODE_ENV=development bun run api.ts",
    "api:uat": "NODE_ENV=uat bun run api.ts",
    "api:prod": "NODE_ENV=production bun run api.ts",
    "dev:all": "concurrently \"bun run api\" \"bun run dev\" --names \"API,NUXT\" --prefix-colors \"blue,green\"",
    "dev:all:uat": "concurrently \"bun run api:uat\" \"bun run dev:uat\" --names \"API,NUXT\" --prefix-colors \"blue,green\"",
    "test": "NODE_ENV=test vitest",
    "test:ui": "NODE_ENV=test vitest --ui",
    "test:run": "NODE_ENV=test vitest run",
    "test:coverage": "NODE_ENV=test vitest run --coverage",
    "test:api": "NODE_ENV=test bun test elysia/",
    "test:all": "bun run test:run && bun run test:api"
  },
  "dependencies": {
    "@elysiajs/cors": "^1.4.0",
    "@elysiajs/jwt": "^1.4.0",
    "@elysiajs/openapi": "^1.4.11",
    "@elysiajs/swagger": "^1.3.1",
    "@tailwindcss/vite": "^4.1.17",
    "bcrypt": "^6.0.0",
    "drizzle-orm": "^0.44.7",
    "elysia": "^1.4.15",
    "nuxt": "^4.2.1",
    "vue": "^3.5.22",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.17",
    "@types/bcrypt": "^6.0.0",
    "@types/bun": "^1.3.2",
    "@vitest/coverage-v8": "4.0.8",
    "@vitest/ui": "^4.0.8",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "daisyui": "^5.4.7",
    "drizzle-kit": "^0.31.6",
    "happy-dom": "^20.0.10",
    "tailwindcss": "^4.1.17",
    "vitest": "^4.0.8"
  }
}
